    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "clean-webpack-plugin": "^0.1.15",
    "cross-env": "^3.1.4",
    "electron": "^1.6.1",
    "electron-prebuilt": "^1.4.13",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-html-plugin": "^0.1.1",
    "webpack-merge": "^3.0.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router": "^3.0.2"
  }
